//bubble way
public class Solution {
    public int findKthLargest(int[] nums, int k) {
        int j = k;
        while(j > 0){
            for(int i = nums.length-1 ; i > 0 ; i--){
                if(nums[i] > nums[i-1]){
                    int temp = nums[i];
                    nums[i] = nums[i-1];
                    nums[i-1] = temp;
                }
            }
            j--;
        }
        return nums[k-1];
    }
}
